						<img class="rounded-circle" src="https://www.themoviedb.org/t/p/w100_and_h100_face/{{.Image}}" alt="{{.Name}}">
					{{end}}
				<div class="status-indicator bg-success"></div>
			</div>
		<div class="font-weight-bold">
		<div class="text-truncate">{{.Name}}</div>
		<div class="small text-white" alt="{{.Department}}">{{.Character}}</div>
		</div>
		</a>
	</div>
	{{end}}
</div>
<div class="col-xl-6 col-lg-6">
<h5 class="h6 mt-0 mb-3 font-weight-bold">CREW</h5>
