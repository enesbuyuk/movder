		{{if eq .SessionBool true}}
		<div class="p-4 bg-opacity rounded mt-4">
		<h5 class="card-title mb-4">Leave a Comment</h5>
		<form id="FilmComment" method="POST" action="/backend/commentfilm">
			<input style="display: none" name="film" id="film" value="{{.Film.Id}}"/>
			<input style="display: none" name="filmtype" id="filmtype" value="{{.Film.FilmType}}"/>
		<div class="row">
		</div>
		<div class="control-group form-group">
		<div class="controls">
		<label>Content <span class="text-danger">*</span></label>
		<textarea rows="3" cols="100" class="form-control" name="content" id="content"></textarea>
		</div>
		</div>
		<div class="text-right">
		<button type="submit" class="btn btn-primary">Send Comment</button>
		</div>
		</form>
		</div>
		{{end}}
</div>
</div>
</div>
</div>
</div>

<div style="clear:both"></div>
</div>
<div class="container">
{{if ne .Similar.TotalResults 0}}
<div class="d-sm-flex align-items-center justify-content-between mt-4 mb-3">
<h1 class="h5 mb-0">{{.Language.Advices}}</h1>
</div>
<div class="row">
<div class="col-xl-12 col-md-12 mb-4">
<div class="collections-slider">
		{{range .Similar.Results}}
		<div class="col-xl-2 col-md-6 mb-4">
			<div class="card m-card shadow border-0">
				<a href="{{$SiteConfig.SiteUrl}}film/{{.Id}}/{{.Url}}">
					<div class="m-card-cover">
							<div class="position-absolute bg-opacity shadow-sm rounded text-center p-2 m-2 love-box-left">
								{{if eq .CheckWatchedlist true}}
								<h5 class="mb-0 font-weight-bold" onclick="watchedlist('{{.Id}}','{{$Film.FilmType}}');"><i id="coverwatchedlist{{.Id}}" class="fa-regular fa-bookmark text-primary"></i></h5>
								{{else}}
								<h5 class="mb-0 font-weight-bold" onclick="watchedlist('{{.Id}}','{{$Film.FilmType}}');"><i id="coverwatchedlist{{.Id}}" class="fa-regular fa-bookmark text-white"></i></h5>
								{{end}}
								<br>
								{{if eq .CheckWatchlist true}}
								<h5 class="mb-0 font-weight-bold" onclick="watchlist('{{.Id}}','{{$Film.FilmType}}');"><i id="coverwatchlist{{.Id}}" class="fa-regular fa-clock text-primary"></i></h5>
								{{else}}
								<h5 class="mb-0 font-weight-bold" onclick="watchlist('{{.Id}}','{{$Film.FilmType}}');"><i id="coverwatchlist{{.Id}}" class="fa-regular fa-clock text-white"></i></h5>
								{{end}}
							</div>
						<div class="position-absolute bg-opacity shadow-sm rounded text-center p-2 m-2 love-box">
							<h6 class="mb-0 font-weight-bold"><i class="fa-solid fa-star text-warning"></i> {{.Vote}}</h6>
							<small>{{.VoteCount}}</small>
						</div>
						{{if eq (len .Image) 0}}
						<img src="{{$SiteConfig.SiteUrl}}static/cdn/default/film.jpg" class="card-img-top" alt="{{.Title}}">
						{{else}}
						<img src="https://www.themoviedb.org/t/p/w220_and_h330_face{{.Image}}" class="card-img-top" alt="{{.Title}}">
						{{end}}
					</div>
					<div class="card-body p-2">
						<h5 class="card-title text-white mb-1">{{.Title}}</h5>
						<p class="card-text"><small class="text-white">{{if eq .Adult true}}For Adult{{else}}No-Adult{{end}}</small> <small class="text-danger ml-2"><i class="fas fa-calendar-alt fa-sm"></i> {{.Date}}</small> </p>
					</div>
				</a>
			</div>
		</div>
		{{end}}
</div>
</div>
</div>
{{end}}
</div>
