{{define "filssm"}}
	{{template "head".}}
	{{template "header".}}
	{{$SiteConfig := .SiteConfig}}
	{{$Film := .Film}}


	<div class="col-xl-3 col-lg-3"></div>
	<div class="col-xl-9 col-lg-9 film_background">
	  <img class="demo-bg" src="https://image.tmdb.org/t/p/w1920_and_h1080_multi_faces{{.Film.Image}}" alt=""></div>
<div class="capsule_film">
<div class="row mb-4 pb-2 list-bp bg-opacity ">
<div class="col-xl-3 col-lg-3">
<div class="pl-3 pt-3 pb-3 widget mb-4">

{{if eq (len .Film.Image) 0}}
<img src="{{$SiteConfig.SiteUrl}}static/cdn/default/film_page.jpg" class="img-fluid rounded" alt="...">
{{else}}
<img src="https://image.tmdb.org/t/p/w500{{.Film.Image}}" class="img-fluid rounded" alt="...">
{{end}}

<h1 class="h6 mb-0 mt-3 font-weight-bold">Director</h1>
	<p>{{range .Credit.Crew}}
		{{if eq .Job "Director"}}
		{{.Name}}&nbsp;&nbsp;&nbsp;
		{{end}}
	{{end}}</p>
<h1 class="h6 mb-0 mt-3 font-weight-bold">Writers</h1>
	<p>{{range .Credit.Crew}}
		{{if eq .Job "Writer"}}
		{{.Name}},
		{{end}}
	{{end}}
	{{range .Credit.Crew}}
		{{if eq .Job "Screenplay"}}
		{{.Name}},
		{{end}}
	{{end}}</p>
<h1 class="h6 mb-0 mt-3 font-weight-bold">Plot</h1>
<p class="mb-0">{{.Film.Overview}}</p>
</div>
</div>
<div class="col-xl-9 col-lg-9">
<div class="info-header mb-4">
<div class="d-flex align-items-center justify-content-between p-3">
<div>
<h3 class="mb-0 mt-0 font-weight-bold">{{.Film.Title}}&nbsp;<small>({{.Film.Date}})</small></h3>
<p class="mb-0 text-gray-800"><small class="text-muted"><i class="fas fa-film fa-fw fa-sm mr-1"></i> Action / Adventure / Science Fiction / 3D</small></p>
</div>
<div>

	{{if ne .Film.FilmBudget "0"}}
<p class="mb-0"><i class="fas fa-money-bill fa-sm fa-fw mr-1"></i> BUDGET: <span class="text-white rounded px-2 py-1 bg-dark">$ {{.Film.FilmBudget}}</span></p>
	{{end}}
	<div class="bg-opacity shadow-sm rounded text-center p-2 mt-4 mr-5 love-box">
<h6 class="mb-0 font-weight-bold"><i class="fa-solid fa-star text-warning"></i> {{.Film.Vote}}</h6>
<small style="color:#ccc">{{.Film.VoteCount}}</small>
</div>

</div>
</div>
<div class="d-flex align-items-center justify-content-between pt-1 pb-3 px-3">
<div class="d-flex">

<style type="text/css">
	.film_icon{color:#FFF;text-align:center;margin-right:10px}
	.film_icon i{font-size:28px;margin-bottom:10px}
	.film_icon_active{color: #9441ff !important}
</style>

	{{if eq .Watchedlist 0}}
	<a id="watchedlist{{.Film.Id}}" href="javascript:watchedlist('{{.Film.Id}}','{{.Film.FilmType}}');" class="d-sm-inline-block film_icon">
		<i class="fa-regular fa-bookmark only_icon"></i><br>Watched
	</a>
	<a id="diary{{.Film.Id}}" href="javascript:watchedlist('{{.Film.Id}}','{{.Film.FilmType}}','diary');" class="film_icon d-none">
		<i class="fa-regular fa-calendar-days only_icon"></i><br>Diary
	</a>
	{{else}}
	<a id="watchedlist{{.Film.Id}}" href="javascript:watchedlist('{{.Film.Id}}','{{.Film.FilmType}}');" class="d-sm-inline-block film_icon film_icon_active">
		<i class="fa-regular fa-bookmark only_icon"></i><br>Watched
	</a>
		{{if eq .Diary 0}}
		<a id="diary{{.Film.Id}}" href="javascript:watchedlist('{{.Film.Id}}','{{.Film.FilmType}}','diary');" class="d-sm-inline-block film_icon">
			<i class="fa-regular fa-calendar-days only_icon"></i><br>Diary
		</a>
		{{else}}
		<a id="diary{{.Film.Id}}" href="javascript:watchedlist('{{.Film.Id}}','{{.Film.FilmType}}','diary');" class="d-sm-inline-block film_icon film_icon_active">
			<i class="fa-regular fa-calendar-days only_icon"></i><br>Diary
		</a>
		{{end}}
	{{end}}

	{{if eq .Favoritelist 0}}
	<a id="favoritelist{{.Film.Id}}" href="javascript:favorite('{{.Film.Id}}','{{.Film.FilmType}}');" class="d-sm-inline-block film_icon">
		<i class="fa-regular fa-heart only_icon"></i><br>Favorite
	</a>
	{{else}}
	<a id="favoritelist{{.Film.Id}}" href="javascript:favorite('{{.Film.Id}}','{{.Film.FilmType}}');" class="d-sm-inline-block film_icon film_icon_active">
		<i class="fa-regular fa-heart only_icon"></i><br>Favorite
	</a>
	{{end}}


	{{if eq .Watchlist 0}}
	<a id="watchlist{{.Film.Id}}" href="javascript:watchlist('{{.Film.Id}}','{{.Film.FilmType}}');" class="d-sm-inline-block film_icon">
		<i class="fa-regular fa-clock only_icon"></i><br>Watchlist
	</a>
	{{else}}
	<a id="watchlist{{.Film.Id}}" href="javascript:watchlist('{{.Film.Id}}','{{.Film.FilmType}}');" class="d-sm-inline-block film_icon film_icon_active">
		<i class="fa-regular fa-clock only_icon"></i><br>Watchlist
	</a>
	{{end}}

&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
	<div>
	<fieldset class="rate">
			<span class="rounded bg-primary text-white pl-1 pr-1"><b>{{.RateCount}}/10</b></span>

	    <span class="ratestar {{if ge .Rate 20}}colorgrad1{{end}}" title="20/2 star" onclick="rate('20','{{.Film.Id}}','{{.Film.FilmType}}')"></span>
	    <span class="ratestar half {{if ge .Rate 19}}colorgrad1{{end}}" title="19/2 star" onclick="rate('19','{{.Film.Id}}','{{.Film.FilmType}}')"></span>

	    <span class="ratestar {{if ge .Rate 18}}colorgrad1{{end}}" title="18/2 star" onclick="rate('18','{{.Film.Id}}','{{.Film.FilmType}}')"></span>
	    <span class="ratestar half {{if ge .Rate 17}}colorgrad1{{end}}" title="17/2 star" onclick="rate('17','{{.Film.Id}}','{{.Film.FilmType}}')"></span>

	    <span class="ratestar {{if ge .Rate 16}}colorgrad1{{end}}" title="16/2 star" onclick="rate('16','{{.Film.Id}}','{{.Film.FilmType}}')"></span>
	    <span class="ratestar half {{if ge .Rate 15}}colorgrad1{{end}}" title="15/2 star" onclick="rate('15','{{.Film.Id}}','{{.Film.FilmType}}')"></span>
	    
	    <span class="ratestar {{if ge .Rate 14}}colorgrad1{{end}}" title="14/2 star" onclick="rate('14','{{.Film.Id}}','{{.Film.FilmType}}')"></span>
	    <span class="ratestar half {{if ge .Rate 13}}colorgrad1{{end}}" title="13/2 star" onclick="rate('13','{{.Film.Id}}','{{.Film.FilmType}}')"></span>

	    <span class="ratestar {{if ge .Rate 12}}colorgrad1{{end}}" title="12/2 star" onclick="rate('12','{{.Film.Id}}','{{.Film.FilmType}}')"></span>
	    <span class="ratestar half {{if ge .Rate 11}}colorgrad1{{end}}" title="11/2 star" onclick="rate('11','{{.Film.Id}}','{{.Film.FilmType}}')"></span>

	    <span class="ratestar {{if ge .Rate 10}}colorgrad1{{end}}" title="10/2 star" onclick="rate('10','{{.Film.Id}}','{{.Film.FilmType}}')"></span>
	    <span class="ratestar half {{if ge .Rate 9}}colorgrad1{{end}}" title="9/2 star" onclick="rate('9','{{.Film.Id}}','{{.Film.FilmType}}')"></span>

	    <span class="ratestar {{if ge .Rate 8}}colorgrad1{{end}}" title="8/2 star" onclick="rate('8','{{.Film.Id}}','{{.Film.FilmType}}')"></span>
	    <span class="ratestar half {{if ge .Rate 7}}colorgrad1{{end}}" title="7/2 star" onclick="rate('7','{{.Film.Id}}','{{.Film.FilmType}}')"></span>

	    <span class="ratestar {{if ge .Rate 6}}colorgrad1{{end}}" title="6/2 star" onclick="rate('6','{{.Film.Id}}','{{.Film.FilmType}}')"></span>
	    <span class="ratestar half {{if ge .Rate 5}}colorgrad1{{end}}" title="5/2 star" onclick="rate('5','{{.Film.Id}}','{{.Film.FilmType}}')"></span>

	    <span class="ratestar {{if ge .Rate 4}}colorgrad1{{end}}" title="4/2 star" onclick="rate('4','{{.Film.Id}}','{{.Film.FilmType}}')"></span>
	    <span class="ratestar half {{if ge .Rate 3}}colorgrad1{{end}}" title="3/2 star" onclick="rate('3','{{.Film.Id}}','{{.Film.FilmType}}')"></span>

	    <span class="ratestar {{if ge .Rate 2}}colorgrad1{{end}}" title="2/2 star" onclick="rate('2','{{.Film.Id}}','{{.Film.FilmType}}')"></span>
	    <span class="ratestar half {{if ge .Rate 1}}colorgrad1{{end}}" title="1/2 star" onclick="rate('1','{{.Film.Id}}','{{.Film.FilmType}}')"></span>
	</fieldset>

	</div>
</div>
<div class="d-flex">
	<a href="javascript:CopyUrl();" class="d-sm-inline-block btn btn-sm btn-primary btn-circle">
		<i class="fas fa-copy only_icon"></i>
	</a>
	<a href="#" class="d-sm-inline-block btn btn-sm btn-dark btn-circle" data-title="{{.Film.Title}}" data-url="{{.SiteConfig.SiteUrl}}" data-sharer="email" data-to="contact@movder.com">
		<i class="fa-solid fa-envelope only_icon"></i>
	</a>
	<a href="#" class="d-sm-inline-block btn btn-sm btn-primary btn-circle" data-title="{{.Film.Title}}" data-url="{{.SiteConfig.SiteUrl}}" data-sharer="facebook">
		<i class="fab fa-facebook-f only_icon"></i>
	</a>
	<a href="#" class="d-sm-inline-block btn btn-sm btn-info btn-circle" data-title="{{.Film.Title}}" data-url="{{.SiteConfig.SiteUrl}}" data-sharer="twitter">
		<i class="fab fa-twitter only_icon"></i>
	</a>
	<a href="#" class="d-sm-inline-block btn btn-sm btn-danger btn-circle" data-title="{{.Film.Title}}" data-url="{{.SiteConfig.SiteUrl}}" data-sharer="instagram">
