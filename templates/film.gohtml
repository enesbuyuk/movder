<div class="tab-content" id="myTabContent">
<div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
<center><h6 class="mt-0 mb-3">“ {{.Film.Tagline}} ”</h6></center>
<h6 class="mt-0 mb-3">Overview</h6>
<p>&nbsp;&nbsp;&nbsp;&nbsp;{{.Film.Overview}}</p>
</div>
<div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
<div class="row">
<div class="col-xl-6 col-lg-6">
<h5 class="h6 mt-0 mb-3 font-weight-bold">CAST</h5>
	{{range .Credit.Cast}}
	<div class="artist-list mb-3">
		<a class="d-flex align-items-center" href="{{$SiteConfig.SiteUrl}}person/{{.Id}}/{{.Url}}">
			<div class="dropdown-list-image mr-3">
					{{if eq (len .Image) 0}}
						<img class="rounded-circle" src="{{$SiteConfig.SiteUrl}}static/cdn/profile/default.jpg" alt="{{.Name}}">
